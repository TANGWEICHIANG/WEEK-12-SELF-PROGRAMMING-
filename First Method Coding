package Method;

public class FirstMethod {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 20;
		String name = "wc";
		System.out.println("I have a message for you");
		showMessage();
		showMessage2();
		System.out.println("That's all buddie!");
		showMessage3(i);
		showMessage4(i,name);
		showMessage5(60, "tension");
	}	//main method
	
	//public static - modifier
	//void - result type (not returning everything)
	//showMessage - method name
	//() - parameter
	public static void showMessage() {
		System.out.println("Hello World!");
	}	//showMessage method
	
	public static void showMessage2() {
		System.out.println("Hello Malaysia!");
	}
	
	public static void showMessage3(int j) {
		System.out.println("The number is " +j);
	}
	
	public static void showMessage4(int k, String x) {
		int m = 4;
		String s = "mlk";
		System.out.println("The number is " +k+ " from " +s);
		System.out.println("My name is "+x);
		int sum = k+m;
		System.out.println("Sum is "+sum);
	}
	
	public static void showMessage5(int age, String y) {
		System.out.println("My age is " +age+ " and I feel " +y);
	}
}
